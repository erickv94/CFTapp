<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/localtramite"
        android:paddingLeft="10dp"
        android:paddingTop="20dp" />

    <Spinner
        android:id="@+id/busqueda1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        />

    <EditText
        android:id="@+id/busqueda2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/fechasolicitudtramite"
        android:inputType="text"
        android:paddingTop="20dp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="10dp">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tramiteread"
            android:id="@+id/imageButton2"
            android:drawableRight="@drawable/ic_show"
            android:onClick="consultarTramite"/>

    </LinearLayout>
    <EditText
        android:id="@+id/idtramite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="id"
        android:enabled="false"
        />
    <EditText
        android:id="@+id/idlocal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/localtramite"
        android:enabled="false"
        />

    <EditText
        android:id="@+id/idtipotramite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/tipotramitetramite"
        android:enabled="false"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fecha"
        android:hint="@string/fechasolicitudtramite"
        android:enabled="false"/>
</LinearLayout>
